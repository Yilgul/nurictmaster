  <div class="row">
    <div class="articlebox ">
      <% @articles.reverse_each do |article| %>
      <div class="articlebox">
        <div class="articleindex">
          <%= link_to article.title, article %>
        </div>
        <div class="articlebody">
          <%= article.text.html_safe %>
        </div>
      <% end %>
      </div>
    </div>
  </div>
